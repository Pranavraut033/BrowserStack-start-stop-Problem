{
  "name": "start-stop-browser",
  "version": "1.0.0",
  "description": "Browserstack round 1 sub 1",
  "main": "dist/index.js",
  "author": "Pranav ",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "set NODE_ENV=testing & mocha --require @babel/register './src/test/*.test.js'",
    "build": "babel src --out-dir dist",
